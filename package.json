{
  "name": "ngx-seo-page",
  "description": "Library that facilitates the management of canonical links, metatags, structured data and the title of web pages.",
  "keywords": [
    "angular",
    "seo",
    "structured data",
    "canonical links",
    "angular universal",
    "ssr"
  ],
  "version": "2.0.1",
  "author": {
    "name": "Rogerio Trist√£o Junior",
    "email": "rogerio.junior@outlook.com",
    "url": "https://github.com/nimesko"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nimesko/ngx-seo-page"
  },
  "homepage": "https://github.com/nimesko/ngx-seo-page",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prebuild": "rimraf ./dist",
    "build": "ng build",
    "copy-license": "copyfiles LICENSE ./dist/ngx-seo-page",
    "copy-readme": "copyfiles README.md ./dist/ngx-seo-page",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm-pack": "cd dist/ngx-seo-page && npm pack",
    "package": "npm run build && npm run copy-files && npm run npm-pack",
    "commit": "git-cz",
    "test": "ng test --code-coverage",
    "test:watch": "ng test --watch --code-coverage",
    "lint": "ng lint",
    "codacy": "cat ./coverage/ngx-seo-page/lcov.info | ./node_modules/.bin/codacy-coverage -p ."
  },
  "dependencies": {
    "@angular/animations": "~8.2.8",
    "@angular/common": "~8.2.8",
    "@angular/compiler": "~8.2.8",
    "@angular/core": "~8.2.8",
    "@angular/forms": "~8.2.8",
    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/router": "~8.2.8",
    "core-js": "^3.2.1",
    "rxjs": "~6.5.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular-devkit/build-ng-packagr": "~0.803.6",
    "@angular/cli": "~8.3.6",
    "@angular/compiler-cli": "~8.2.8",
    "@angular/language-service": "~8.2.8",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.7.7",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "^5.0.1",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "ts-node": "~8.4.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.9.3",
    "tslint": "~5.20.0",
    "typescript": "^3.5.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
