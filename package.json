{
  "name": "ngx-seo-page",
  "description": "Library that facilitates the management of canonical links, metatags, structured data and the title of web pages.",
  "keywords": [
    "angular",
    "seo",
    "structured data",
    "canonical links",
    "angular universal",
    "ssr"
  ],
  "version": "1.0.1",
  "author": {
    "name": "Rogerio TristÃ£o Junior",
    "email": "rogerio.junior@outlook.com",
    "url": "https://github.com/nimesko"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nimesko/ngx-seo-page"
  },
  "homepage": "https://github.com/nimesko/ngx-seo-page",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf ./dist",
    "build": "ng build ngx-seo-page",
    "copy-license": "copy LICENSE ./dist/ngx-seo-page",
    "copy-readme": "copy README.md ./dist/ngx-seo-page",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm-pack": "cd dist/ngx-seo-page && npm pack",
    "package": "npm run build && npm run copy-files && npm run npm-pack",
    "commit": "git-cz",
    "test": "ng test ngx-seo-page --code-coverage",
    "test:watch": "ng test ngx-seo-page --watch --code-coverage",
    "lint": "ng lint ngx-seo-page",
    "e2e": "ng e2e",
    "codacy": "cat ./coverage/ngx-seo-page/lcov.info | ./node_modules/.bin/codacy-coverage -p ."
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.9",
    "@angular-devkit/build-ng-packagr": "~0.13.9",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@commitlint/cli": "^7.6.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.0.0",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "~5.0.1",
    "commitizen": "^3.1.1",
    "copy": "^0.3.2",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "~8.1.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.16.0",
    "typescript": "~3.2.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
