{
  "name": "ngx-seo-page",
  "description": "Library that facilitates the management of canonical links, metatags, structured data and the title of web pages.",
  "keywords": [
    "angular",
    "seo",
    "structured data",
    "canonical links",
    "angular universal",
    "ssr"
  ],
  "version": "3.0.1",
  "author": {
    "name": "Rogerio Trist√£o Junior",
    "email": "rogerio.junior@outlook.com",
    "url": "https://github.com/nimesko"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nimesko/ngx-seo-page"
  },
  "homepage": "https://github.com/nimesko/ngx-seo-page",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prebuild": "rimraf ./dist",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "copy-license": "copyfiles LICENSE ./dist/ngx-seo-page",
    "copy-readme": "copyfiles README.md ./dist/ngx-seo-page",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm-pack": "cd dist/ngx-seo-page && npm pack",
    "package": "npm run build && npm run copy-files && npm run npm-pack",
    "commit": "git-cz",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^15.0.2",
    "@angular/common": "^15.0.2",
    "@angular/compiler": "^15.0.2",
    "@angular/core": "^15.0.2",
    "@angular/forms": "^15.0.2",
    "@angular/platform-browser": "^15.0.2",
    "@angular/platform-browser-dynamic": "^15.0.2",
    "@angular/router": "^15.0.2",
    "rxjs": "~7.5.0",
    "schema-dts": "^1.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.0.1",
    "@angular-devkit/build-angular": "^15.1.4",
    "@angular/cli": "^15.0.2",
    "@angular/compiler-cli": "^15.0.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "^29.0.1",
    "commitizen": "^4.2.5",
    "copyfiles": "^2.4.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "ng-packagr": "^14.2.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.8.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
